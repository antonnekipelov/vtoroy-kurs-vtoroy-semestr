Var a,b,c,d:real;
Begin
Write('Введите a ');
Readln(a);
Write('Ведите b ');
Readln(b);
Write('Ведите c ');
Readln(c);
d:=(a+b)/c+c/(a*b)+245;
writeln(d:0:3);
end.
